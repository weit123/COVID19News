<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.java.weitong">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/appicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".StartupActivity"
            android:label="CoKnow">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.news.NewsLoadActivity"
            android:label="新闻详情">
        </activity>
        <activity
            android:name=".ui.data.DataDisplayActivity"
            android:label="加载数据图表">
        </activity>
        <activity
            android:name=".ui.news.ShareActivity"
            android:label="分享页面">
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name=".ui.scholar.ScholarContentActivity"
            android:label="学者信息">

        </activity>

        <meta-data android:name="DATABASE" android:value="covid19_news.db" />
        <meta-data android:name="VERSION" android:value="2" />
        <meta-data android:name="QUERY_LOG" android:value="true" />
        <meta-data android:name="DOMAIN_PACKAGE_NAME" android:value="com.java" />

    </application>

</manifest>